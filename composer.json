{
    "name":"magento-hackathon/magento-composer-installer",
    "description":"Composer installer for Magento modules and core",
    "keywords":[ "composer-installer", "magento"],
    "minimum-stability":"dev",
    "type":"composer-plugin",
    "license":"OSL-3.0",
    "homepage":"https://github.com/SlayerBirden/magento-composer-installer",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        }
    ],
    "authors":[
        {
            "name":"Daniel Fahlke aka Flyingmana",
            "email":"flyingmana@googlemail.com"
        },
        {
            "name":"JÃ¶rg Weller",
            "email":"weller@flagbit.de"
        },
        {
            "name":"Karl Spies",
            "email":"karl.spies@gmx.net"
        },
        {
            "name":"Tobias Vogt",
            "email":"tobi@webguys.de"
        },
        {
            "name":"David Fuhr",
            "email":"fuhr@flagbit.de"
        },
        {
            "name":"Vinai Kopp",
            "email":"vinai@netzarbeiter.com"
        },
        {
            "name":"Oleg Kulik",
            "email":"olegkulik1985@gmail.com"
        }
    ],
    "require":{
        "php": ">=5.4",
        "eloquent/composer-config-reader": "2.*",
        "symfony/console": "~2.5",
        "composer-plugin-api": "1.0.0"
    },
    "require-dev":{
        "phpunit/phpunit":"~4.3",
        "phpunit/phpunit-mock-objects": "~2.3",
        "squizlabs/php_codesniffer": "~2.1",
        "cotya/composer-test-framework": "dev-master",
        "composer/composer":"1.0.*@dev",
        "symfony/process":"~2.5",
        "mikey179/vfsStream":"~1.4"
    },
    "suggest":{
        "theseer/autoload": "~1.14",
        "colinmollenhour/modman": "*"
    },
    "autoload":{
        "psr-0":{
            "MagentoHackathon\\Composer":"src/"
        }
    },
    "autoload-dev":{
        "psr-0":{
            "MagentoHackathon\\Composer\\Magento":"tests/"
        }
    },
    "bin": [
        "bin/magento-composer-installer.php",
        "bin/installer_core_utils.php"
    ],
    "archive": {
        "exclude": [
            "vendor",
            "/tests/FullStackTest/"
        ]
    },
    "extra":{
        "class":"MagentoHackathon\\Composer\\Magento\\Plugin"
    }
}
